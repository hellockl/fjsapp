page {
    height: 100%;
    background:#fff;
}
.scroll-view_H{
    white-space:nowrap;
    width: 100%;
    background-color: rgba(255, 255, 255, 0.7);
}

.banner-scroll{
    margin-top:2px;
}

.banner-scroll repeat{
    margin-bottom:10px;
}

.nav-name{
    display:inline-block;
    margin:0 5px;
    font-size:16px;
    color: #2b2e33;
    border-bottom: 2px solid transparent;
    padding:10px;
}

.nav-hover{
    color: #f06000;
    border-bottom: 2px solid #f06000;
}


.navbar {
    display: flex;
    background: #fff;
    height: 80rpx;
}

.navbar .item {
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 80rpx;
    margin:0 10px
}

.navbar .item.active {
    color: #31c27c;
}

.navbar .item.active:after {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #31c27c;
}



/* swiper轮播 */

.recommend .swiper {
    height: 380rpx;
}

.recommend .swiper .img {
    width: 100%;
    height: 100%;
}

.wx-swiper-dot::before {
    color: #fff;
}

.wx-swiper-dot-active::before {
    color: green;
}

/* 工具 */


.recommend .channel {
    margin: 0;
}

.recommend .channel .title {
    display: flex;
    margin: 20rpx 20rpx;
}

.recommend .channel .list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.recommend .channel .list .item {
    background: #fff;
}

.recommend .channel .list .item .img {
    position: relative;
    vertical-align: top;
    width: 350rpx;
    height: 350rpx;
}

.recommend .channel .list .item .list-media {
    position: relative;
    width: 350rpx;
    height: 350rpx;
    box-sizing: border-box;
    overflow: hidden;
}

.recommend .channel .list .item .list-media:after {
    content: '';
    position: absolute;
    right: 10rpx;
    bottom: 10rpx;
    width: 48rpx;
    height: 48rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.recommend .channel .list .item .list-media .list-count {
    position: absolute;
    left: 10rpx;
    bottom: 10rpx;
    font-size: 24rpx;
    color: #fff;
    z-index: 10;
}
.clear{
  clear: both
}
.recommend .channel .list .item .list-media .list-count:before {
    display: inline-block;
    margin-right: 6rpx;
    content: '';
    width: 20rpx;
    height: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.recommend .channel .list .item .text {
    display: block;
    padding: 10rpx 14rpx;
    font-size: 28rpx;
    width: 322rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.recommend .channel .list .songitem {
    margin-bottom: 20rpx;
}

.recommend .channel .list .author {
    display: block;
    padding: 0rpx 14rpx 10rpx;
    font-size: 24rpx;
}


.nav-box {
    width: 100%;
}
.nav-box .item {
    float: left;
    padding: 0.66667rem 0 0.66667rem 0.4rem;
    width: 31%;
    color: #333;
    font-size: 24rpx;
}

.nav-box .item image {
    float: left;
    margin-right: 0.13333rem;
    width: 120rpx;
    height: 120rpx;
}

img {
    width: 100%;
    display: block;
    vertical-align: middle;
}



.topList .item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 0rpx;
    background: #fff;
}

.topList .media {
    flex: 0 0 200rpx;
    position: relative;
    width: 200rpx;
    height: 200rpx;
}

.topList .media .img {
    margin: 10rpx; 
    width: 200rpx;
    height: 180rpx;
}

.topList .media .count {
    position: absolute;
    bottom: 10rpx;
    left: 10rpx;
    font-size: 24rpx;
    line-height: 1;
    color: #fff;
    opacity: 0.6;
}

.topList .media .count:before {
    display: inline-block;
    margin-right: 6rpx;
    content: '';
    width: 20rpx;
    height: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.topList .info {
    flex: auto;
    margin-left: 30rpx;
    overflow: hidden;
}

.topList .info .title {
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #000;
    margin-bottom: 10rpx;
}

.topList .info .text {
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 28rpx;
    color: rgba(0, 0, 0, 0.5);
}

.topList .info .text text {
    color: #000;
    margin-left: 16rpx;
    margin-right: 10rpx;
}

.topList .arrow {
    flex: 0 0 12rpx;
    margin-right: 20rpx;
    width: 12rpx;
    height: 12rpx;
    border-right: 1px solid #b2b2b2;
    border-bottom: 1px solid #b2b2b2;
    transform: rotate(-45deg);
}

 .nav-box1 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 20rpx;
    background: #fff;
}

.fd-bd-title {
    padding-left: 20rpx;
    margin-top: 40rpx;
    margin-bottom: 20rpx;
    font-size: 32rpx;
    font-weight: bold;
    color: #444;
    border-left: 10rpx solid #03a5ef;
}
.fd-bd {
    padding: 0 20rpx 20rpx;
}


.fd-bd-person {
    display: flex;
    width: 100%;
    height: 280rpx;
    overflow-x: scroll;
    overflow-y: hidden;
}

.fd-bd-person-item {
    margin-left: 20rpx;
    text-align: center;
}

.fd-bd-person-item:first-child {
    margin-left: 0;
}

.fd-bd-person-avatar {
    width: 170rpx;
    height: 170rpx;
}

.fd-bd-person-name {
    color: #666;

}

.fd-bd-person-role {
    color: #999
}
